<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <Description>Add EntityFramework Core IQueryable support to GraphQL</Description>
    <PolyNullability>true</PolyNullability>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="ConfigureAwait.Fody" Version="3.3.2" PrivateAssets="All" />
    <PackageReference Include="Fody" Version="6.8.0" PrivateAssets="all" />
    <PackageReference Include="Microsoft.AspNetCore.Http" Version="2.2.2" />
    <PackageReference Include="GraphQL" Version="7.8.0" />
    <PackageReference Include="Microsoft.EntityFrameworkCore" Version="8.0.3" />
    <PackageReference Include="Polyfill" Version="4.2.0" PrivateAssets="all" />
    <PackageReference Include="ProjectDefaults" Version="1.0.126" PrivateAssets="all" />
    <!-- explicit to avoid a CVE -->
    <PackageReference Include="System.Text.Encodings.Web" Version="8.0.0" />
    <Using Include="System.Linq.Expressions" />
    <Using Include="GraphQL.Types" />
    <Using Include="Microsoft.EntityFrameworkCore" />
    <Using Include="System.Diagnostics.CodeAnalysis" />
    <Using Include="GraphQL" />
    <Using Include="GraphQL.Types.Relay.DataObjects" />
    <Using Include="GraphQLParser.AST" />
    <Using Include="GraphQL.EntityFramework" />
    <Using Include="GraphQL.Resolvers" />
    <Using Include="GraphQL.Types.Relay" />
    <Using Include="System.Globalization" />
    <Using Include="GraphQL.Execution" />
    <Using Include="GraphQL.Builders" />
    <Using Include="System.Text.RegularExpressions" />
  </ItemGroup>
</Project>